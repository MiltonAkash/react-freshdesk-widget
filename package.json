{
  "name": "react-freshdesk-widget",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "description": "A React component library for integrating Freshdesk widgets",
  "keywords": [
    "react",
    "freshdesk",
    "widget",
    "customer-support",
    "helpdesk"
  ],
  "author": "Akash Milton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiltonAkash/react-freshdesk-widget.git"
  },
  "main": "dist/freshdesk-widget.cjs",
  "module": "dist/freshdesk-widget.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/freshdesk-widget.js",
      "require": "./dist/freshdesk-widget.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  }
}
